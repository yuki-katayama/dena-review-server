{
  "name": "dena-review-server",
  "version": "1.0.0",
  "description": "DenaRewview server",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "pprebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "postbuild": "yarn copyStaticFiles",
    "prestart": "yarn build",
    "start": "node dist/server.js",
    "copyStaticFiles": "copyfiles -u 2 src/static/* dist"
  },
  "dependencies": {
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "http": "^0.0.1-security",
    "mongoose": "^6.1.1",
    "socket.io": "^4.4.0",
    "typescript": "^4.5.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/cors": "^2.8.12",
    "@types/mongoose": "^5.11.97",
    "@types/socket.io": "^3.0.2",
    "@types/socket.io-parser": "^3.0.0",
    "@types/uuid": "^8.3.0",
    "tslint": "^6.1.3"
  }
}
